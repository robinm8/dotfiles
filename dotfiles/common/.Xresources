*background: #000
*foreground: #eee

! Default
! 0: black
*color0:  #222222
*color8:  #222222
! 1: red
*color1:  #9f6767
*color9:  #9f6767
! 2: green
*color2:  #92ac68
*color10: #92ac68
! 3: yellow
*color3:  #d0d293
*color11: #d0d293
! 4: blue
*color4:  #9aacc3
*color12: #9aacc3
! 5: magenta
*color5:  #bb77a4
*color13: #bb77a4
! 6: cyan
*color6:  #77bbb5
*color14: #77bbb5
! 7: white
*color7:  #777777
*color15: #777777

Xft.dpi: 180

URxvt.internalBorder: 19
URxvt.cursorBlink: true
URxvt.scrollBar: false
URxvt*letterSpace: -1
URxvt.savelines:        500

URxvt*scrollTtyOutput:  false
URxvt*scrollWithBuffer: true
URxvt*scrollTtyKeypress: true

URxvt.perl-ext-common:default,clipboard
URxvt.keysym.M-c:perl:clipboard:copy
URxvt.keysym.M-v:perl:clipboard:paste

URxvt.perl-ext-common: default,matcher
URxvt.url-launcher: icecat
URxvt.matcher.button: 1

URxvt.font:xft:Roboto:size=10

Xcursor.size: 32
